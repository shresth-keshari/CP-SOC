// Function to find square root
// x: element to find square root
class Solution{
  public:
    long long int floorSqrt(long long int x) 
    {
        for (long long int i=0; i<x; i++){
            if(x==2){
                return 1;
            }
            if (i*i>x){
                return i-1;
            }
        }
        // Your code goes here   
    }
};
